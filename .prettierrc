{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": true,
  "semi": true,
  "trailingComma": "all",
  "bracketSpacing": true,            
  "jsxBracketSameLine": false,       
  "arrowParens": "always",           
  "endOfLine": "lf",                 
  "htmlWhitespaceSensitivity": "css",
  "quoteProps": "as-needed",         
